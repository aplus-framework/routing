{
    "name": "aplus/routing",
    "type": "library",
    "description": "Aplus Framework Routing Library",
    "keywords": [
        "routing",
        "router",
        "resource",
        "presenter",
        "rest",
        "restfull"
    ],
    "homepage": "https://docs.aplus-framework.com/guides/libraries/routing/",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "aplus/http": "^4.0",
        "aplus/language": "^1.5"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.9",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Routing\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/routing/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/routing/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/routing",
        "docs": "https://docs.aplus-framework.com/guides/libraries/routing/"
    }
}
