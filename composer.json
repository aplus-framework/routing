{
    "name": "aplus/routing",
    "description": "Aplus Framework Routing Library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "routing",
        "router",
        "resource",
        "presenter",
        "rest",
        "restfull"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/routing",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://github.com/aplus-framework/routing/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://github.com/aplus-framework/routing",
        "docs": "https://docs.aplus-framework.com/guides/libraries/routing/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.1",
        "aplus/debug": "^3.1",
        "aplus/http": "^5.0",
        "aplus/language": "^3.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.14",
        "ergebnis/composer-normalize": "^2.25",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Routing\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
